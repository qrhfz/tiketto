  </div>
  <!-- Modal Update -->
  <div class="modal fade" id="modalUpdateTransaksi" tabindex="-1" role="dialog"
    aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLongTitle">Status</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <form id="form-update-transaksi" method="POST" class="mt-5">
            <div class="row justify-content-center">
              <div class="form-group col-12 col-sm-12 col-md-6">
                <div class="form-group text-center w-100">
                  <input type="text" name="status" id="status" class="form-control">
                  <small class="text-muted form-text pt-2">Menunggu Konfirmasi / Diproses / Selesai</small>
                </div>
              </div>
            </div>
            <div class="modal-footer justify-content-center">
              <input type="submit" class="btn btn-info" value="Edit Transaksi">
              <button type="button" class="btn btn-secondary" data-dismiss="modal">Batal</button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
  <!-- Modal Info -->
  <div class="modal fade" id="modalDetailTransaksi" tabindex="-1" role="dialog"
    aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLongTitle">Info Transaksi</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <form method="POST">
            <div class="row">
              <div class="form-group col-12 col-sm-12 col-md-6">
                <div class="form-group">
                  <label>Nama barang</label>
                  <input type="text" name="nama_barang" id="nama_barang" class="form-control">
                </div>
              </div>
              <div class="form-group col-12 col-sm-12 col-md-6">
                <label>Pengiriman/Pengambilan</label>
                <div class="input-group">
                  <div class="input-group-prepend">
                    <div class="input-group-text">
                      <i class="fas fa-boxes"></i>
                    </div>
                  </div>
                  <input type="text" name="pengiriman" id="pengiriman" class="form-control">
                </div>
              </div>
            </div>
            <div class="row">
              <div class="form-group col-12 col-sm-12 col-md-6">
                <label>Pembayaran</label>
                <div class="input-group">
                  <div class="input-group-prepend">
                    <div class="input-group-text">
                      <i class="fas fa-money-bill-alt"></i>
                    </div>
                  </div>
                  <input type="text" name="nama_barang" id="pembayaran" class="form-control">
                </div>
              </div>
              <div class="form-group col-12 col-sm-12 col-md-6">
                <label>Pengembalian</label>
                <div class="input-group">
                  <div class="input-group-prepend">
                    <div class="input-group-text">
                      <i class="fas fa-boxes"></i>
                    </div>
                  </div>
                  <input type="text" name="nama_barang" id="pengembalian" class="form-control">
                </div>
              </div>
            </div>
            <div class="row">
              <div class="form-group col-12 col-sm-12 col-md-6">
                <div class="form-group">
                  <label>Tanggal Sewa</label>
                  <input name="tanggal_sewa" type="date" id="tanggal_sewa" class="form-control datepicker">
                  </di>
                </div>
              </div>
              <div class="form-group col-12 col-sm-12 col-md-6">
                <div class="form-group">
                  <label>Tanggal Kembali</label>
                  <input name="tanggal_kembali" id="tanggal_kembali" type="date" class="form-control datepicker">
                </div>
              </div>
            </div>
            <div class="modal-footer">
              <input type="submit" class="btn btn-info" value="Buat Transaksi">
              <button type="button" class="btn btn-secondary" data-dismiss="modal">Batal</button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>

  <script>
    const btnInfo = document.getElementsByTagName('tr')
    let idx, res
    for (idx = 0; idx < btnInfo.length; idx++) {
      let a = btnInfo[idx + 1].getElementsByTagName('td')[11].getElementsByTagName('button')[0]
      a.addEventListener('click', () => {
        res = a.getAttributeNode('data-id').textContent.split(',')
        document.getElementById('nama_barang').setAttribute('value', res[0])
        document.getElementById('pengiriman').setAttribute('value', res[1].split('/', 1))
        document.getElementById('pengembalian').setAttribute('value', res[1].split('/', 2, 1)[1])
        document.getElementById('pembayaran').setAttribute('value', res[2])
        document.getElementById('tanggal_sewa').setAttribute('value', res[3])
        document.getElementById('tanggal_kembali').setAttribute('value', res[4])
      })
    }
  </script>

  <script>
    const id = document.getElementById('get-id').getAttribute('data-id')
    const btnUpdate = document.getElementsByTagName('tr')
    let j, result
    for (j = 0; j < btnUpdate.length; j++) {
      let a = btnUpdate[j + 1].getElementsByTagName('td')[11].getElementsByTagName('button')[1]
      a.addEventListener('click', () => {
        result = a.getAttributeNode('data-id').textContent.split(',')
        console.log(result)
        document.getElementById('form-update-transaksi').setAttribute('action', `/admin/${id}/transaksi/${result[1]}`)
        document.getElementById('status').setAttribute('value', result[0])
      })
    }
  </script>


